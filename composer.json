{
    "name": "yqueue/api-support",
    "license": "proprietary",
    "description": "A package of useful classes and utilities to assist with backend application development.",
    "type": "library",
    "authors": [
        {
            "name": "Luke Vear",
            "email": "lukemvear@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "YQueue\\ApiSupport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "illuminate/contracts": "^5.8 | ^6.0",
        "illuminate/database": "5.7 - 5.8 | ^6.0",
        "illuminate/http": "^5.8 | ^6.0",
        "ramsey/uuid": "^3.8",
        "moontoast/math": "^1.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YQueue\\ApiSupport\\ApiSupportServiceProvider"
            ]
        }
    }
}
